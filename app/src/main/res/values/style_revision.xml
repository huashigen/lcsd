<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 更多菜单列表RelativeLayout -->
    <style name="style_more_menu_item_rl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">18dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="style_more_menu_item_rl2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">18dp</item>
        <item name="android:paddingRight">18dp</item>
        <item name="android:paddingTop">12dp</item>

        <item name="android:clickable">true</item>
    </style>
    <!-- 理财金菜单列表RelativeLayout -->
    <style name="style_licaijin_item_rl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:clickable">true</item>
    </style>
    <!-- 边框背景 -->
    <!--         <item name="android:background">@drawable/list_item_bg</item> -->

    <!-- 更多菜单列表左侧TextView -->
    <style name="style_more_item_left_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#8E8E8E</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <!-- 更多菜单列表右侧ImageView（向右图标） -->
    <style name="style_more_item_right_iv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:src">@drawable/triangle</item>
    </style>

    <!-- 猜你喜欢底部介绍文字 -->
    <style name="style_home_guess_bottom_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#959595</item>
    </style>

    <style name="style_home_guess_bottom_text_top" parent="@style/style_home_guess_bottom_text">
        <item name="android:textSize">@dimen/dimens_home_guess_grid_item_text</item>
    </style>

    <style name="style_home_guess_bottom_text_bottom" parent="@style/style_home_guess_bottom_text">
        <item name="android:textSize">12sp</item>
    </style>

    <!-- 多途径重置密码LinearLayout -->
    <style name="style_forget_pwd_channel_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/bg_forget_pwd_channel_item</item>
        <item name="android:padding">15dp</item>
        <item name="android:clickable">true</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>

    <!-- 多途径重置密码TextView -->
    <style name="style_forget_pwd_channel_tv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <!-- 多途径重置密码iv -->
    <style name="style_forget_pwd_channel_iv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/triangle</item>
    </style>

    <!-- 扁平化方角list列表LinearLayout -->
    <style name="style_square_list_item_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:padding">12dp</item>
        <item name="android:background">@drawable/bg_forget_pwd_channel_item</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:clickable">true</item>
    </style>

    <!-- 扁平化方角list列表左侧图标ImageView -->
    <style name="style_square_list_item_iv_icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">8dp</item>
    </style>

    <!-- 扁平化方角list列表文字TextView -->
    <style name="style_square_list_item_tv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">#888888</item>
    </style>

    <!-- 扁平化方角list列表向右箭头ImageView -->
    <style name="style_square_list_item_iv_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:src">@drawable/triangle</item>
    </style>

    <!-- 扁平化方角list列表文字数据TextView -->
    <style name="style_square_list_item_tv_data">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#858585</item>
    </style>

    <!-- 充值提现键盘每一行LinearLayout -->
    <style name="style_keyboard_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- 充值提现键盘按钮button -->
    <style name="style_keyboard_bt">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/keyboard_square_bg</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- 充值提现详细信息页面 每行LinearLayout -->
    <style name="style_rw_info_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">12dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@color/white</item>
    </style>

    <!-- 充值提现详细信息页面 左侧TextView -->
    <style name="style_rw_info_left_tv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#8E8E8E</item>
    </style>

    <!-- 充值提现详细信息页面 右侧TextView -->
    <style name="style_rw_info_right_tv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.4</item>
        <item name="android:textSize">14sp</item>
    </style>

    <!-- 充值提现详细信息页面 黑线View -->
    <style name="style_rw_info_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#DDDDDD</item>
    </style>

    <!-- 自定义progressbar  loading -->
    <style name="style_loading_v1">
        <item name="android:indeterminateDrawable">@drawable/loading_rotate_v1</item>
    </style>

    <!-- 预约 -->
    <style name="style_bespeak_apply_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#666666</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="style_bespeak_apply_item">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/bg_square_border</item>
        <item name="android:padding">6dp</item>
    </style>

    <style name="style_bespeak_apply_bar">
        <item name="android:layout_width">15dp</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#666666</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_marginRight">12dp</item>
    </style>

    <style name="style_bespeak_area_list_tv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <!-- 消息中心pop -->
    <style name="style_message_centre_pop_tv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
    </style>

    <style name="style_message_centre_pop_div">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:gravity">center</item>
        <item name="android:background">#BBB9B9</item>
    </style>


    <!-- 消息中心pop  (new)-->
    <style name="style_message_centre_pop_item_ll">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="style_message_centre_pop_line_vertical">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">#E4E4E4</item>
    </style>

    <style name="style_message_centre_pop_line_horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#E4E4E4</item>
    </style>

    <style name="style_message_centre_pop_img">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="style_message_centre_pop_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@drawable/bg_account_dimension_tv</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>

    <!-- 消息理财 -->
    <style name="style_newfinance_bottom_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#959595</item>
        <item name="android:textSize">10sp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <!-- 财富页面 -->
    <style name="style_account_dimension_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="style_account_dimension_rl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="style_account_dimension_img">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>

    </style>

    <style name="style_account_dimension_number">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/bg_account_tip_num</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_gravity">right|top</item>
        <item name="android:visibility">gone</item>
    </style>


    <style name="style_account_dimension_number_1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/dot_msg</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="style_account_dimension_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@drawable/bg_account_dimension_tv</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="style_account_describe_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">end</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:drawableRight">@drawable/triangle</item>
        <item name="android:drawablePadding">4dp</item>
    </style>

    <style name="style_withdrawals_item_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bg_corner_border_gray</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="style_withdrawals_item_iv_icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="style_withdrawals_item_tv_left">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/dimens_withdrawals_item_text_left</item>
        <item name="android:textColor">#333333</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="style_withdrawals_item_angle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
</resources>

